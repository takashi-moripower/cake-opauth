{
    "name": "takashi-moripower/cake-opauth",
    "description": "CakeOpauth plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
		"opauth/opauth": "*",
		"opauth/facebook": "*",
		"opauth/google": "*",
		"opauth/twitter": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "CakeOpauth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeOpauth\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
